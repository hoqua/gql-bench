{
  "name": "chat-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "env-cmd -f .local.env concurrently --kill-others \"npm:db:up\" \"npm:dev:api\" \"npm:dev:web\"",
    "dev:api": "nx serve api",
    "dev:web": "nx serve web",
    "DATABASE": "-----------------------------------------------------------------------------------------------",
    "db:up": "npm run db:docker && npm run db:dev-migrate && npm run db:create-client -- --watch",
    "db:docker": "docker-compose -f tools/chat-docker-env/docker-compose.yml up -d --no-recreate --remove-orphans",
    "db:create-client": "prisma generate",
    "db:dev-migrate": "env-cmd --no-override -f .local.env npx prisma migrate dev",
    "db:studio": "env-cmd --no-override -f .local.env npx prisma studio"
  },
  "prisma": {
    "schema": "libs/api/data-access/db/src/lib/schema.prisma"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "^14.5.4",
    "@nrwl/cypress": "14.5.4",
    "@nrwl/eslint-plugin-nx": "14.5.4",
    "@nrwl/jest": "14.5.4",
    "@nrwl/linter": "14.5.4",
    "@nrwl/nest": "^14.5.4",
    "@nrwl/next": "^14.5.4",
    "@nrwl/node": "14.5.4",
    "@nrwl/react": "14.5.4",
    "@nrwl/web": "14.5.4",
    "@nrwl/workspace": "14.5.4",
    "@testing-library/react": "13.3.0",
    "@types/async": "^3.2.15",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-is": "17.0.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "27.5.1",
    "babel-plugin-styled-components": "1.10.7",
    "concurrently": "^7.3.0",
    "cypress": "^10.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "27.5.1",
    "nx": "14.5.4",
    "prettier": "^2.6.2",
    "prisma": "^4.1.1",
    "prisma-nestjs-graphql": "^17.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/mercurius": "^10.0.21",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.0.8",
    "@prisma/client": "^4.1.1",
